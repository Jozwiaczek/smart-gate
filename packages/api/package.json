{
  "name": "api",
  "version": "1.0.3",
  "private": true,
  "scripts": {
    "prebuild": "yarn clean",
    "build": "nest build",
    "clean": "rimraf build",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache",
    "lint:fix": "yarn lint --fix",
    "serve:coverage": "serve -s ./coverage -l 4002",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node build/src/main",
    "test": "jest --runInBand",
    "test:cov": "yarn test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "yarn test --watch",
    "type-check": "tsc --noEmit",
    "typeorm": "ts-node ../../node_modules/typeorm/cli.js",
    "typeorm-migration:run": "yarn typeorm migration:run"
  },
  "dependencies": {
    "@nestjs/cli": "^8.1.6",
    "@nestjs/common": "^8.2.4",
    "@nestjs/core": "^8.2.4",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/platform-socket.io": "^8.2.4",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.2.4",
    "@sentry/node": "^6.16.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^3.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mjml": "^4.7.0",
    "@types/ms": "^0.7.31",
    "@types/uuid": "^8.3.3",
    "@types/web-push": "^3.3.2",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mjml": "^4.11.0",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.1",
    "ts-loader": "^9.2.6",
    "tsconfig-paths": "^3.12.0",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "web-push": "^3.4.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.2.4",
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2"
  }
}
