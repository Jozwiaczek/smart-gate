{
  "name": "api",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "prebuild": "yarn clean",
    "build": "nest build",
    "clean": "rimraf build",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache",
    "lint:fix": "yarn lint --fix",
    "serve:coverage": "serve -s ./coverage -l 4002",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node build/src/main",
    "test": "jest --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "type-check": "tsc --noEmit",
    "typeorm": "ts-node ../../node_modules/typeorm/cli.js",
    "typeorm-migration:run": "yarn typeorm migration:run"
  },
  "dependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/mapped-types": "^0.3.0",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/throttler": "^1.1.1",
    "@nestjs/typeorm": "^7.1.5",
    "@sentry/node": "^6.2.2",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.11.0",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mjml": "^4.7.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^14.14.6",
    "@types/nodemailer": "^6.4.1",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "mjml": "^4.9.0",
    "ms": "^2.1.3",
    "nodemailer": "^6.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.30",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.5.1",
    "@types/jest": "^26.0.15",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.3",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3"
  }
}
