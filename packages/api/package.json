{
  "name": "api",
  "version": "1.0.3",
  "private": true,
  "scripts": {
    "prebuild": "yarn clean",
    "build": "nest build",
    "clean": "rimraf build",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache",
    "lint:fix": "yarn lint --fix",
    "serve:coverage": "serve -s ./coverage -l 4002",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node build/src/main",
    "test": "jest --runInBand",
    "test:cov": "yarn test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "yarn test --watch",
    "type-check": "tsc --noEmit",
    "typeorm": "ts-node ../../node_modules/typeorm/cli.js",
    "typeorm-migration:run": "yarn typeorm migration:run"
  },
  "dependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/common": "^7.6.17",
    "@nestjs/core": "^7.6.17",
    "@nestjs/mapped-types": "^0.4.1",
    "@nestjs/platform-express": "^7.6.17",
    "@nestjs/platform-socket.io": "^7.6.17",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/throttler": "^1.1.4",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.17",
    "@sentry/node": "^6.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^3.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.11.1",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mjml": "^4.7.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.4.6",
    "@types/nodemailer": "^6.4.2",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^8.3.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.4.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mjml": "^4.9.3",
    "ms": "^2.1.3",
    "nodemailer": "^6.6.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.34",
    "typescript": "^4.3.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.6.17",
    "@types/jest": "^26.0.23",
    "@types/supertest": "^2.0.11",
    "jest": "^27.0.4",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3"
  }
}
