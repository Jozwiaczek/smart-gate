{
  "name": "client",
  "version": "1.0.3",
  "private": true,
  "scripts": {
    "prebuild": "yarn clean",
    "build": "react-scripts build",
    "build:storybook": "build-storybook",
    "clean": "rimraf build",
    "hygen:element": "hygen element new && cd ../.. && yarn prettier:fix && yarn lint:fix",
    "hygen:icon": "hygen icon new && cd ../.. && yarn prettier:fix && yarn lint:fix",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache",
    "lint:fix": "yarn lint --fix",
    "serve:build": "yarn build && serve -s build -l 4000",
    "serve:coverage": "yarn test:coverage && serve -s ./coverage -l 4001",
    "start": "react-scripts start",
    "start:storybook": "yarn test && set STORYBOOK=true && start-storybook -s ./public -p 6006",
    "test": "react-scripts test --watchAll=false --passWithNoTests --json --outputFile=./src/jest-test-results.json",
    "test:coverage": "yarn test --coverage",
    "test:watch": "react-scripts test",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@sentry/react": "^6.6.0",
    "@sentry/tracing": "^6.6.0",
    "animejs": "^3.2.1",
    "axios": "^0.21.1",
    "i18next": "^20.3.1",
    "i18next-browser-languagedetector": "^6.1.1",
    "intersection-observer": "^0.12.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.8.4",
    "react-i18next": "^11.11.0",
    "react-query": "^3.17.0",
    "react-router-dom": "^5.2.0",
    "socket.io-client": "^2.4.0",
    "styled-components": "^5.3.0",
    "web-vitals": "^2.0.1",
    "workbox-background-sync": "^6.1.5",
    "workbox-broadcast-update": "^6.1.5",
    "workbox-cacheable-response": "^6.1.5",
    "workbox-core": "^6.1.5",
    "workbox-expiration": "^6.1.5",
    "workbox-google-analytics": "^6.1.5",
    "workbox-navigation-preload": "^6.1.5",
    "workbox-precaching": "^6.1.5",
    "workbox-range-requests": "^6.1.5",
    "workbox-routing": "^6.1.5",
    "workbox-strategies": "^6.1.5",
    "workbox-streams": "^6.1.5"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.3.4",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-jest": "^6.3.4",
    "@storybook/addon-storysource": "^6.3.4",
    "@storybook/addons": "^6.3.4",
    "@storybook/components": "^6.3.4",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.4",
    "@storybook/theming": "^6.3.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.0",
    "@types/animejs": "^3.1.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-query": "^1.2.9",
    "@types/react-router-dom": "^5.1.7",
    "@types/socket.io-client": "^3.0.0",
    "@types/styled-components": "^5.1.10",
    "react-scripts": "^4.0.3",
    "serve": "^12.0.0",
    "typescript": "^4.2.3"
  }
}
