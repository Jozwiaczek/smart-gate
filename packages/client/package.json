{
  "name": "client",
  "version": "1.0.3",
  "private": true,
  "scripts": {
    "prebuild": "yarn clean",
    "build": "react-scripts build",
    "build:storybook": "build-storybook",
    "clean": "rimraf build",
    "hygen:element": "hygen element new && cd ../.. && yarn prettier:fix && yarn lint:fix",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache",
    "lint:fix": "yarn lint --fix",
    "serve:build": "serve -s build -l 4000",
    "serve:coverage": "serve -s ./coverage -l 4001",
    "start": "react-scripts start",
    "start:storybook": "yarn test && STORYBOOK=true start-storybook -s ./public -p 6006",
    "test": "react-scripts test --watchAll=false --passWithNoTests --json --outputFile=./src/jest-test-results.json",
    "test:coverage": "yarn test --coverage",
    "test:watch": "react-scripts test",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@sentry/react": "^6.2.5",
    "@sentry/tracing": "^6.2.5",
    "animejs": "^3.2.1",
    "axios": "^0.21.1",
    "i18next": "^20.1.0",
    "i18next-browser-languagedetector": "^6.1.0",
    "intersection-observer": "^0.12.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^6.14.1",
    "react-i18next": "^11.8.12",
    "react-query": "^3.13.4",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.3",
    "web-vitals": "^1.1.1",
    "workbox-background-sync": "^6.1.2",
    "workbox-broadcast-update": "^6.1.2",
    "workbox-cacheable-response": "^6.1.2",
    "workbox-core": "^6.1.2",
    "workbox-expiration": "^6.1.2",
    "workbox-google-analytics": "^6.1.2",
    "workbox-navigation-preload": "^6.1.2",
    "workbox-precaching": "^6.1.2",
    "workbox-range-requests": "^6.1.2",
    "workbox-routing": "^6.1.2",
    "workbox-strategies": "^6.1.2",
    "workbox-streams": "^6.1.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.2.3",
    "@storybook/addon-actions": "^6.2.3",
    "@storybook/addon-essentials": "^6.2.3",
    "@storybook/addon-jest": "^6.2.3",
    "@storybook/addon-links": "^6.2.3",
    "@storybook/addon-storysource": "^6.2.3",
    "@storybook/addons": "^6.2.3",
    "@storybook/components": "^6.2.3",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.3",
    "@storybook/theming": "^6.2.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.1",
    "@types/animejs": "^3.1.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-query": "^1.1.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "chromatic": "^5.7.1",
    "react-scripts": "^4.0.3",
    "serve": "^11.3.2",
    "typescript": "^4.2.3"
  }
}
